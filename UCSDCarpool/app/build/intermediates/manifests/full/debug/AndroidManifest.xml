<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.menu_ui.menu_ui"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="23" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.menu_ui.menu_ui.Drive_set"
            android:label="@string/title_activity_drive_set"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.menu_ui.menu_ui.Menu_activity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.menu_ui.menu_ui.Log_in"
            android:label="@string/title_activity_log_in"
            android:theme="@style/AppTheme.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.menu_ui.menu_ui.Register"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.menu_ui.menu_ui.passengerUI"
            android:label="@string/title_activity_passenger_ui"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>